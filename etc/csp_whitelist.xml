<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="adobeio_static_css" type="host">*.adobeio-static.net</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="adobeio_js" type="host">*.adobeio-static.net</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="adobe_io" type="host">*.adobe.io</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>