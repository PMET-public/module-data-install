<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="exportpage">
                <settings>
                    <url path="datainstall/export/page"/> <!-- your custom controller to handle request -->
                    <type>exportpage</type>
                    <label translate="true">Export Content</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
